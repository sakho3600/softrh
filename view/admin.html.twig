                    </ul>
                </li>
            </ul>
        </nav>
        <!--/. NAV TOP  -->
        <nav class="navbar-default navbar-side" role="navigation">
            <div class="sidebar-collapse">
                    <ul class="nav" id="main-menu">
                        <li>
                            <a href="/admin" class="active-menu"><i class="fa fa-bar-chart-o"></i>Tout les services</a>
                        </li>
                        {% for service in services %}
                            <li>
                                <a href="/admin/service/{{ service.id }}" class="active-menu"><i class="fa fa-bar-chart-o"></i> {{ service.nom }}</a>
                            </li>
                        {% endfor %}
                    </ul>
            </div>
        </nav>
        <!-- /. NAV SIDE  -->
        <div id="page-wrapper">
            <div id="page-inner">
                <!-- /. ROW  -->
                <div class="row row-legend ">
                    {% for humeur in listHumeur %}
                        <div class="col-lg-3 padding-sm-emoji">
                            <div class="row bg-graphics marge-right ">
                                <div class="col-lg-6 padding-sm-emoji">
                                    <p><img href="#" class="img-fluid " src="{{ humeur.emoticone }}" alt="image" width="90"
                                    ></p>
                                </div>
                                <div class="col-lg-6 padding-sm-emoji">
                                    <p class="text-legend">{{humeur.nom |convert_encoding('UTF-8', 'iso-8859-1')}}</p>
                                    <div class="{{ humeur.class_color}}"></div>
                                </div>
                            </div>
                        </div>
                    {% endfor %}   
                </div>
                <div class="row ">
                    <div class="col-xs-12 col-sm-12 col-md-12 bg-graphics margin-chart">
                        <div class="">
                            <div class="panel-heading">
                                <div>Humeur: Jour 
                            </div>
                            <div class="">
                                <div id="morris-bar-chart"></div>
                            </div>
                        </div>
                    </div>

                    <div class="col-xs-12 col-sm-12 col-md-12 bg-graphics margin-chart">
                        <div class="">
                            <div class="panel-heading">
                                Humeur: Mois
                            </div>
                            <div class="panel-body">
                                <div id="morris-line-chart"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
