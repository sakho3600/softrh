    <script src="/hybrid-admin-free-web-template/assets/js/jquery-1.10.2.js"></script>
    <!-- Bootstrap Js -->
    <script src="/hybrid-admin-free-web-template/assets/js/bootstrap.min.js"></script>
    <!-- Metis Menu Js -->
    <script src="/hybrid-admin-free-web-template/assets/js/jquery.metisMenu.js"></script>
    <!-- Morris Chart Js -->
    <script src="/hybrid-admin-free-web-template/assets/js/morris/raphael-2.1.0.min.js"></script>
    <script src="/hybrid-admin-free-web-template/assets/js/morris/morris.js"></script>
    <!-- Custom Js -->
    <script src="/hybrid-admin-free-web-template/assets/js/custom-scripts.js"></script>



